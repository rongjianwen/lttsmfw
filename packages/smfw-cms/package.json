{
    "name": "@smfw/cms",
    "libname": "SmfwCMS",
    "version": "0.1.0",
    "main": "./src/index.ts",
    "typedocMain": "./src/index.ts",
    "license": "UNLICENSED",
    "scripts": {
        "start:test": "cross-env NODE_ENV=development webpack serve --config webpack.config.js",
        "build:test": "rimraf build/test && cross-env NODE_ENV=production webpack --config webpack.config.js",
        "build:umd": "rimraf build/umd && npm-run-all build:umd:dev build:umd:prod",
        "build:umd:dev": "rimraf build/umd/*.development*.js && cross-env NODE_ENV=development rollup -c rollup.config.js",
        "build:umd:prod": "rimraf build/umd/*.production*.js && cross-env NODE_ENV=production rollup -c rollup.config.js",
        "build:types": "rimraf types && tsc -d --declarationDir types --emitDeclarationOnly",
        "build:all": "npm-run-all build:types build:umd build:test",
        "lint": "eslint . --ext=.jsx,.js,.tsx,.ts,.json,.cjs",
        "lint:fix": "yarn lint:js --fix",
        "precommit": "lint-staged",
        "pretty:config": "prettier --write \"*.{json,cjs,js}\"",
        "pretty:src": "prettier --write \"src/**/*.{js,jsx,ts,tsx,html,css,scss}\"",
        "pretty:all": "npm-run-all pretty:config pretty:src"
    },
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/eslint-parser": "^7.16.3",
        "@babel/plugin-proposal-class-properties": "^7.16.0",
        "@babel/preset-env": "^7.16.4",
        "@babel/preset-react": "^7.16.0",
        "@babel/preset-typescript": "^7.16.0",
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-eslint": "^8.0.1",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^11.0.1",
        "@rollup/plugin-replace": "^3.0.0",
        "@rollup/plugin-yaml": "^3.0.0",
        "@typescript-eslint/eslint-plugin": "^5.5.0",
        "@typescript-eslint/parser": "^5.5.0",
        "babel-loader": "^8.2.3",
        "cross-env": "^7.0.3",
        "css-loader": "^6.5.1",
        "dotenv": "^10.0.0",
        "dotenv-webpack": "^7.0.3",
        "eslint": "^8.4.1",
        "eslint-config-airbnb": "^19.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.27.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "lint-staged": "^12.1.2",
        "mini-css-extract-plugin": "^2.4.5",
        "node-sass": "^7.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.5.0",
        "process": "^0.11.10",
        "rimraf": "^3.0.2",
        "rollup": "^2.35.1",
        "rollup-plugin-cleanup": "^3.2.1",
        "rollup-plugin-external-globals": "^0.6.1",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-summary": "^1.3.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.29.0",
        "sass-loader": "^12.4.0",
        "style-loader": "^3.3.1",
        "stylelint": "^14.1.0",
        "ts-loader": "^9.2.6",
        "typescript": "^4.5.2",
        "webpack": "^5.64.1",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.5.0",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "@material-ui/core": "^4.12.3",
        "@reduxjs/toolkit": "^1.6.2",
        "@types/lodash": "^4.14.177",
        "@types/material-ui": "^0.21.12",
        "@types/react": "^17.0.35",
        "@types/react-dom": "^17.0.11",
        "@types/redux-logger": "^3.0.9",
        "lodash": "^4.17.21",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.6",
        "react-router-dom": "^6.0.2",
        "redux": "^4.1.2",
        "redux-devtools-extension": "^2.13.9",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.4.1"
    }
}
